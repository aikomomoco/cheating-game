class Student {

  int sSize = 20;
  int sX;
  int sY;

  Student(int x, int y) {
    sX = x;
    sY = y;
  }

  void display() {
    fill(28, 213, 12);
    ellipse(sX, sY, sSize*2, sSize*2);
    fill(255, 24, 54);
    ellipse(sX-sSize, int(sY-sSize/2), sSize, sSize);
    ellipse(sX+sSize, int(sY-sSize/2), sSize, sSize);
    fill(36, 36, 36);
    ellipse(sX-sSize, int(sY-sSize/2), sSize/2, sSize/2);
    ellipse(sX+sSize, int(sY-sSize/2), sSize/2, sSize/2);
  }

  void move() {

  }

  
}
